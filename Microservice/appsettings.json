{
  "Kestrel": {
    "Endpoints": {
      "Https": { "Url": "https://localhost:55555" },
      "Http": { "Url": "http://localhost:44444" }
    }
  },

  "ConnectionStrings": {
    "PostgreSqlIdentityDB": "Server=127.0.0.1; Port=5432; Database=IdentityDB; User Id=postgres; Password=postgres;"
  },

  "Upload": {
    "LocalPhysicalPath": "Files/UploadedFiles",
    "AllowedFileExtensions": [ "txt", "json" ]
  },

  "Download": {
    "LocalPhysicalPath": "Files/UploadedFiles",
    "MIMEMapping": {
      "txt": "text/plain",
      "zip": "application/zip",
      "json": "application/json"
    }
  },

  "HashSettings": {
    "KEY_SIZE": 32,
    "SALT_SIZE": 16,
    "ITERATIONS": 10000
  },

  "Jwt": {
    "Issuer": "BackendApp",
    "Audience": "FrontendApp",
    "TokenValidityMinutes": 30,
    "PrivateKeyPkcs8": [
      "-----BEGIN PRIVATE KEY-----",
      "",
      "-----END PRIVATE KEY-----"
    ],
    "PublicKeyX_509": [
      "-----BEGIN PUBLIC KEY-----",
      "",
      "-----END PUBLIC KEY-----"
    ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*"
}
