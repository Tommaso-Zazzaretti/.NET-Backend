{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "PostgreSqlIdentityDB": "Server=127.0.0.1; Port=5432; Database=IdentityDB; User Id=postgres; Password=postgres;"
  },

  "HashSettings": {
    "KEY_SIZE": 32,
    "SALT_SIZE": 16,
    "ITERATIONS": 10000
  },

  "Upload": {
    "LocalPath": "UPLOAD_",
    "AllowedFileExtensions": [ "txt", "json" ]
  },

  "AllowedHosts": "*"
}
